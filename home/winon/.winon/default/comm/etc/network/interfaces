# interfaces(5) file used by ifup(8) and ifdown(8)
auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
  address 10.50.2.15
  network 10.50.2.0
  netmask 255.255.255.0
  broadcast 10.50.2.255
  gateway 10.50.2.2

auto eth1
iface eth1 inet static
  address 10.50.1.1
  network 10.50.1.0
  netmask 255.255.255.0
  broadcast 10.50.1.255
